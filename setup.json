    "ConfTools-z7",
    "QuickApp-z7",
    "SystemCmd-z7",
    "compmake-z7",
    "coverage",
    "junit-xml",
    "nose",
    "reprep-z7",
    "zuper-commons-z7",
    "zuper-html-z7",
